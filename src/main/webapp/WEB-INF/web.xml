<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
 http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
 id="WebApp_ID" version="3.0">
 <display-name>ServletHelloWorld</display-name>
 <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
 </welcome-file-list>
 
 
 
<servlet>
  <servlet-name>HelloForm</servlet-name>
  <servlet-class>servelet.contrllor.HelloForm</servlet-class>
</servlet>
 
<servlet-mapping>
  <servlet-name>HelloForm</servlet-name>
  <url-pattern>/HelloForm</url-pattern>
</servlet-mapping>
 <servlet>
  <servlet-name>HelloForm1</servlet-name>
  <servlet-class>servelet.contrllor.HelloForm1</servlet-class>
</servlet>
 
<servlet-mapping>
  <servlet-name>HelloForm1</servlet-name>
  <url-pattern>/HelloForm1</url-pattern>
</servlet-mapping>
<servlet>
  <servlet-name>HelloForm2</servlet-name>
  <servlet-class>servelet.contrllor.HelloForm2</servlet-class>
</servlet>
 
<servlet-mapping>
  <servlet-name>HelloForm2</servlet-name>
  <url-pattern>/HelloForm2</url-pattern>
</servlet-mapping>
</web-app>